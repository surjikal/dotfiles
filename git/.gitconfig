[user]
    name = Nicolas Porter
    email = porter.nicolas@gmail.com
[gui]
    commitmsgwidth = 120
    tabsize = 4
	fontdiff = -family Menlo -size 14 -weight normal -slant roman -underline 0 -overstrike 0
	spellingdictionary = none
[core]
    editor = nano
[help]
    # Automatically correct and execute mistyped commands
    autocorrect = 1
[url "git@bitbucket.org:42debut/"]
    insteadOf = "42:"
[url "git@github.com:"]
    insteadOf = "gh:"
    pushInsteadOf = "github:"
    pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
    insteadOf = "github:"
[url "git@gist.github.com:"]
    insteadOf = "gst:"
    pushInsteadOf = "gist:"
    pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
    insteadOf = "gist:"
[submodule]
	recurse = true
