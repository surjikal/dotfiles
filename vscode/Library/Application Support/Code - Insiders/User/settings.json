{
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "editor.suggestSelection": "first",
    "editor.snippetSuggestions": "bottom",
    "search.useIgnoreFiles": false,
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "window.restoreWindows": "one",
    "workbench.editor.limit.value": 50,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "vscode-kubernetes.minikube-path.mac": "",

    // General
    "files.associations": {
        "*.xml": "xml",
        "*.gitconfig": "ini",
        "*.txt": "plaintext",
        "*.yaml.enc": "yaml"
    },
    "task.saveBeforeRun": "never",
    "debug.javascript.codelens.npmScripts": "never",
    "debug.javascript.usePreview": false,
    "debug.saveBeforeStart": "none",
    "debug.javascript.automaticallyTunnelRemoteServer": false,
    "security.workspace.trust.untrustedFiles": "open",

    // JS / TS
    "[typescript]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
        "editor.formatOnSave": true,
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "javascript.format.semicolons": "insert",
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.tsserver.maxTsServerMemory": 30000,
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.tsserver.useSeparateSyntaxServer": true,
    "tslint.jsEnable": true,
    "typescript.tsserver.log": "off",
    "typescript.check.npmIsInstalled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": false
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": false
    },
    // Misc
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[graphql]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "terraform.languageServer": {
        "external": false,
        "args": [
            "serve"
        ]
    },


    // Python
    "python.analysis.memory.keepLibraryAst": true,
    "python.insidersChannel": "daily",
    "python.autoComplete.addBrackets": true,
    "python.jediMemoryLimit": -1,
    "python.analysis.completeFunctionParens": true,
    "python.languageServer": "Pylance",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },

    // Jupyter (trying to disable it)
    "jupyter.sendSelectionToInteractiveWindow": false,
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.jupyterLaunchTimeout": 1,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },

    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "git-rebase-todo": "default" // disable gitlens association
    },

    // Extensions
    "sops.beta": true,
    "edi-x12.telemetry.disabled": true,
    "tctoolbox.core": {
        "ssoActiveTokenUri": "",
        "ssoClientId": ""
    },
    "tctoolbox.x12": {
        "eppServiceUri": ""
    },

    // Gitlens
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.advanced.similarityThreshold": 60,
    "gitlens.blame.ignoreWhitespace": true,

    // Editor Settings
    "editor.cursorStyle": "line-thin",
    "editor.fontFamily": "Menlo, monospace",
    "editor.fontLigatures": false,
    "editor.fontSize": 20,
    "editor.minimap.enabled": false,
    "editor.folding": false,
    "editor.renderControlCharacters": true,

    // Theme settings
    "breadcrumbs.enabled": true,
    "breadcrumbs.icons": false,
    "debug.console.fontSize": 14,
    "material-icon-theme.folders.theme": "classic",
    "materialTheme.accent": "Blue",
    "scm.countBadge": "off",
    "workbench.colorTheme": "Base16 Tomorrow",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontFamily": "MesloLGS NF, Menlo, monospace",
    "terminal.integrated.fontSize": 17,
    "workbench.editor.limit.enabled": true,
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "workbench.colorCustomizations": {
        "activityBar.activeBorder": "#2979FF",
        "activityBar.activeFocusBorder": "#ff000000",
        "activityBar.background": "#1A1E2C",
        "activityBar.border": "#ff000000",
        "activityBar.foreground": "#afbcc9",
        "activityBar.inactiveForeground": "#343d46",
        "activityBarBadge.background": "#2979FF",
        "activityBarBadge.foreground": "#00000000",
        "contrastActiveBorder": "#ff000000",
        "contrastBorder": "#22273900",
        // Dropdown
        "dropdown.background": "#212535",
        // Input
        "input.background": "#212535",
        "input.border": "#1A1E2C",
        // Hover Panel
        "editorHoverWidget.background": "#212535",
        "editorHoverWidget.border": "#212535",
        // Find
        "editorWidget.background": "#1A1E2C",
        "editorWidget.border": "#2979FF",
        "editorWidget.resizeBorder": "#2979FF",
        // Autocomplete popup
        "editorSuggestWidget.border": "#00000000",
        "editorSuggestWidget.foreground": "#67737e",
        "editorSuggestWidget.background": "#212535",
        "editorSuggestWidget.selectedBackground": "#212535",
        // CMD+P panel
        "quickInput.background": "#212535",
        // Breadcrumb (hidden)
        "breadcrumb.activeSelectionForeground": "#2979FF",
        "breadcrumb.background": "#16171900",
        "breadcrumb.focusForeground": "#AFBCC900",
        "breadcrumb.foreground": "#67737e00",
        // Editor
        "editor.background": "#1A1E2C",
        "editor.findMatchBackground": "#2979FF70",
        "editor.findMatchBorder": "#005eff00",
        "editor.findMatchHighlightBackground": "#2979FF70",
        "editor.hoverHighlightBackground": "#2979FF00",
        "editor.lineHighlightBackground": "#2979FF00",
        "editor.lineHighlightBorder": "#2979FF00",
        "editor.rangeHighlightBackground": "#2979FF00",
        "editor.selectionBackground": "#2979FF50", // mouse selection
        "editor.selectionHighlightBackground": "#e1f22c15", // click selection
        "editor.selectionHighlightBorder": "#e1f22cA0",
        "editorBracketMatch.border": "#343d46",
        "editorCursor.foreground": "#67737e",
        "editorCodeLens.foreground": "#67737e",
        "editorGroupHeader.tabsBackground": "#1A1E2C",
        "editorIndentGuide.activeBackground": "#343d4680",
        "editorIndentGuide.background": "#343d4680",
        "editorLineNumber.foreground": "#343d4680",
        "editorSuggestWidget.highlightForeground": "#2979FF",
        "editorGroup.border": "#222739",
        // Sidebar
        "list.activeSelectionBackground": "#222739",
        "list.activeSelectionForeground": "#2979FF",
        "list.errorForeground": "#cd6592",
        "list.highlightForeground": "#2979FF",
        "list.hoverBackground": "#282D3F",
        "list.hoverForeground": "#afbcc9",
        "list.inactiveFocusBackground": "#1A1E2C",
        "list.inactiveSelectionBackground": "#222739",
        "list.inactiveSelectionForeground": "#2979FF",
        "menu.selectionForeground": "#2979FF",
        "menubar.selectionForeground": "#2979FF",
        "notificationLink.foreground": "#2979FF",
        "panel.border": "#222739",
        "panelTitle.activeBorder": "#2979FF",
        "pickerGroup.foreground": "#2979FF",
        "progressBar.background": "#2979FF",
        // Status bar
        "statusBar.background": "#222739",
        // Scrollbar
        "scrollbar.shadow": "#00000040",
        "scrollbarSlider.activeBackground": "#2979FF50",
        "scrollbarSlider.background": "#afbcc910",
        "scrollbarSlider.hoverBackground": "#00000000",
        "selection.background": "#2979FF",
        "settings.headerForeground": "#2979FF",
        "settings.modifiedItemIndicator": "#2979FF",
        "sideBar.background": "#1A1E2C",
        "sideBarSectionHeader.background": "#1A1E2C",
        "sideBarSectionHeader.border": "#1C1F2600",
        "sideBarSectionHeader.foreground": "#67737e",
        // Tabs
        "tab.activeBackground": "#1A1E2C",
        "tab.activeBorder": "#2979FF",
        "tab.activeForeground": "#AFBCC9",
        "tab.activeModifiedBorder": "#2979FF",
        "tab.unfocusedActiveBorder": "#2979FF",
        "tab.border": "#222739",
        "tab.hoverBackground": "#343d4640",
        "tab.inactiveBackground": "#1A1E2C",
        "tab.inactiveModifiedBorder": "#FF0000",
        "tab.unfocusedActiveModifiedBorder": "#FF0000",
        "tab.unfocusedHoverBorder": "#343d4640",
        "textLink.foreground": "#2979FF",
        "titleBar.activeBackground": "#1A1E2C",
        "titleBar.activeForeground": "#343d4600",
        "titleBar.border": "#22273910",
        "tree.indentGuidesStroke": "#1C1F26",
        "widget.shadow": "#FF000000",
        // "editor.findRangeHighlightBackground": "#ff0000",
        // "editor.findRangeHighlightBorder": "#ff0000",
        // "editor.symbolHighlightBackground": "#ff0000",
        // "editor.wordHighlightBackground": "#ff0000",
        // "sideBar.border": "#222739",
        // Terminal Colors
        // "terminal.foreground": "#ebebeb",
        "terminal.background": "#1A1E2C",
        "terminalCursor.background": "#ffffff",
        "terminalCursor.foreground": "#c9a8fa",
        "terminal.ansiBlack": "#272936",
        "terminal.ansiBlue": "#caa9fa",
        "terminal.ansiBrightBlack": "#686868",
        "terminal.ansiBrightBlue": "#caa9fa",
        "terminal.ansiBrightCyan": "#9aedfe",
        "terminal.ansiBrightGreen": "#5af78e",
        "terminal.ansiBrightMagenta": "#ff92d0",
        "terminal.ansiBrightRed": "#ff807b",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiBrightYellow": "#f4f99d",
        "terminal.ansiCyan": "#9aedfe",
        "terminal.ansiGreen": "#5af78e",
        "terminal.ansiMagenta": "#ff92d0",
        "terminal.ansiRed": "#cd6592",
        "terminal.ansiWhite": "#c7c7c7",
        "terminal.ansiYellow": "#f4f99d"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "meta.function-call.generic.python",
                "settings": {
                    "foreground": "#cd6592"
                }
            },
            {
                "scope": "variable.parameter.function",
                "settings": {
                    "foreground": "#e68fb5"
                }
            }
        ]
    },

    "workbench.activityBar.visible": true,
    "extensions.ignoreRecommendations": false,
    "search.quickOpen.includeHistory": false,
    // "search.sortOrder": "fileNames",
    "explorer.compactFolders": false,
    "editor.inlineSuggest.enabled": true,
    "timeline.excludeSources": [
        "git-history"
    ],

    // Trying to disable
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.mac": "xxxxxxxxxxxxxxxxxx"
    },
    // Trying to disable
    "remote.containers.dockerPath": "xxxxxxxxxxxxxxxxxx",

    "redhat.telemetry.enabled": false,
    "sqltools.useNodeRuntime": true,
    "sqltools.connections": [],

    "github.copilot.enable": {
        "*": false,
        "yaml": true,
        "plaintext": false,
        "markdown": false,
        "typescript": true,
        "terraform": true,
        "coffeescript": true,
        "shellscript": true,
        "python": true,
        "applescript": true
    },
    "window.zoomLevel": -1
}
