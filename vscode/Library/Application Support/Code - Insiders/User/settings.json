{
    "search.exclude": {
        "**/.venv": true,
        "**/.git": true,
        "**/state": true,
        "**/node_modules": true,
        "**/package-lock.json": true,
        "**/.cache": true,
        "**/.sass-cache": true,
        "**/.ultra.cache.json": true,
        "**/.yarn": true,
        "**/yarn-error.log": true,
        "**/tsconfig.tsbuildinfo": true,
        "**/.terraform/**": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "**/build": true,
        "**/build/**/*": true,
        "**/node_modules": true,
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.venv/**": true,
        "**/.tsbuildinfo": true,
        "**/__pycache__": true,
        "**/metastore_db": true,
        "**/.spark-warehouse": true,
        "**/.ipynb_checkpoints": true,
        "**/.pytest_cache": true,
        "**/.trunk/*out": true
    },
    "debug.openDebug": "neverOpen",
    "debug.focusWindowOnBreak": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "search.useIgnoreFiles": false,
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.minVisible": 0,
    "window.restoreWindows": "one",

    "terminal.integrated.inheritEnv": false,
    "window.autoDetectHighContrast": false,

    "window.title": "${dirty}${activeEditorShort}${separator}${folderPath}${separator}${appName}",

    // General
    "files.associations": {
        "*.xml": "xml",
        "*.gitconfig": "ini",
        "*.txt": "plaintext",
        "*.yaml.enc": "yaml",
        "git-rebase-todo": "git-rebase",
        ".dockerignore": "ignore",
        "*.sh": "shellscript",
        ".node_history": "plaintext",
        ".gcloudignore": "ignore"
    },

    "task.saveBeforeRun": "never",
    "debug.saveBeforeStart": "none",
    "debug.javascript.codelens.npmScripts": "never",
    "debug.javascript.automaticallyTunnelRemoteServer": false,
    "security.workspace.trust.untrustedFiles": "open",

    "editor.formatOnType": false,

    // JS / TS
    "typescript.suggest.paths": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.tsserver.maxTsServerMemory": 16000,
    "typescript.workspaceSymbols.scope": "currentProject",
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.tsserver.log": "off",
    "typescript.surveys.enabled": false,
    "typescript.tsserver.enableTracing": false,
    "typescript.check.npmIsInstalled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.format.semicolons": "insert",

    "toTypeScript.coffeescript.optionalChaining": true,
    "toTypeScript.coffeescript.useJSModules": true,
    "toTypeScript.coffeescript.useCS2": true,

    "coffeesense.useWorkspaceDependencies": true,

    "eslint.ignoreUntitled": true,
    "eslint.probe": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "vue",
        "markdown",
        "yaml",
        "coffeescript"
    ],
    "eslint.useESLintClass": true,
    "eslint.debug": true,
    "eslint.format.enable": true,
    "eslint.migration.2_x": "off",
    "lintel.ignoredDirectories": [
        "state",
        "coverage",
        "tmp",
        ".tmp",
        ".parcel-cache",
        "node_modules",
        "build",
        "dist",
        "out"
    ],

    "prettier.enableDebugLogs": false,
    "jestrunner.preserveEditorFocus": true,
    "jestrunner.configPath": "jest.config.js",
    "jestrunner.changeDirectoryToWorkspaceRoot": false,
    "jestrunner.codeLensSelector": "**/*.{test,spec}.{js,jsx,ts,tsx,mts,mjs,cjs,cts}",
    "editor.inlineSuggest.showToolbar": "always",

    "[typescript][typescriptreact][javascript][javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": "never",
            "source.fixAll.eslint": "never",
            "source.organizeImports": "never"
        }
    },

    "[coffeescript]": {
        // "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": {
            "source.fixAll": "never",
            "source.organizeImports": "never"
        }
    },

    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
    },

    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": false
    },

    // Misc
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[graphql]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[git-rebase]": {
        "editor.suggest.showWords": false,
        "editor.suggest.showSnippets": false,
        "workbench.editor.restoreViewState": false
    },

    // Python
    "python.venvPath": ".venv",
    "python.languageServer": "Pylance",
    "python.analysis.autoFormatStrings": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.userFileIndexingLimit": -1,
    "python.analysis.enablePytestSupport": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.enablePytestExtra": true,
    "python.testing.pytestArgs": ["--capture=tee-sys"],
    "python.testing.pytestEnabled": true,
    "pythonTestExplorer.testFramework": "pytest",
    "ruff.nativeServer": true,

    // https://github.com/microsoft/vscode-python/blob/main/src/client/common/experiments/groups.ts
    "python.experiments.optInto": [
        "All"
        // "pythonSurveyNotification",
        // "pythonDiscoveryUsingWorkers",
        // "pythonPromptNewToolsExt"
    ],
    "[python]": {
        "editor.formatOnType": false,
        "editor.wordBasedSuggestions": "off",
        "editor.suggestSelection": "first",
        "editor.snippetSuggestions": "top",
        // "editor.defaultFormatter": "ms-python.python",
        "editor.defaultFormatter": "eeyore.yapf",
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
          "source.organizeImports": "explicit",
          "source.fixAll": "never"
        }
    },

    // Jupyter (trying to disable it)
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.allowUnauthorizedRemoteConnection": true,
    "jupyter.enableExtendedPythonKernelCompletions": true,
    "jupyter.executionAnalysis.enabled": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },

    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "git-rebase-todo": "default" // disable gitlens association
    },

    // Extensions
    "sops.beta": true,
    "edi-x12.telemetry.disabled": true,
    "tctoolbox.core": {
        "ssoActiveTokenUri": "",
        "ssoClientId": ""
    },
    "tctoolbox.x12": {
        "eppServiceUri": ""
    },
    "gitlens.advanced.similarityThreshold": 60,
    "gitlens.blame.ignoreWhitespace": true,

    // Editor Settings
    "editor.stickyScroll.enabled": false,
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.cursorWidth": 1,
    "window.zoomLevel": -1,
    "editor.fontFamily": "Menlo, Comic Sans MS",
    "editor.fontLigatures": false,
    "editor.fontSize": 19,
    "editor.experimental.asyncTokenization": true,
    "editor.folding": false,
    "editor.renderControlCharacters": true,
    "editor.inlayHints.enabled": "off",
    "window.experimental.useSandbox": true,
    "breadcrumbs.icons": false,
    "debug.console.fontSize": 14,
    "scm.countBadge": "off",


    "materialTheme.accent": "Blue",

    "oneDarkPro.vivid": false,
    "oneDarkPro.italic": false,

    "workbench.editor.limit.value": 1000,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.enableExperiments": true,

    "editor.semanticHighlighting.enabled": false,

