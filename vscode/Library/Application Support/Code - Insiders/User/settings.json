    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "meta.function-call.generic.python",
                "settings": {
                    "foreground": "#cd6592"
                }
            },
            {
                "scope": "variable.parameter.function",
                "settings": {
                    "foreground": "#e68fb5"
                }
            }
        ]
